idProduct}`);\n        setChange(!change);\n        return;\n    }\n    const { img1, name, description, idProduct, price, ubicacion } = dataProduct;\n    return (\n\n        <ProductContainer>\n            <ImageContainer>\n                <ImageComponent src={img1} />\n            </ImageContainer>\n            <Title>{name}</Title>\n            <Description>\n                <LinesEllipsis\n                    text={description}\n                    maxLine='2'\n                />\n            </Description>\n            <Price >${price} x da</Price>\n            <Location >{ubicacion}</Location>\n            <ButtonContainer>\n                <Button onClick={() => window.location.href = `${url}/producto/${idProduct}`} children={`${isMyProduct ? \"Ver publicacin\" : \"Ver detalles\"}`} />\n\n            </ButtonContainer>\n            {isMyProduct && <Controls>\n                {\n                    dataProduct.idStatus === \"1\" ? <IconPause><FontAwesomeIcon data-tip=\"Pausar publicacion\" icon={faPause} onClick={() => pausarPublicacion()} /></IconPause>\n                        :\n                        <IconPlay><FontAwesomeIcon data-tip=\"Reanudar publicacion\" icon={faPlay} onClick={() => reanudarPublicacion()} /></IconPlay>\n\n                }\n                <IconDelete><FontAwesomeIcon data-tip=\"Eliminar publicacion\" icon={faTrash} onClick={() => eliminarPublicacion()} /></IconDelete>\n                <ReactTooltip place=\"top\" type=\"dark\" effect=\"solid\" />\n            </Controls>}\n\n        </ProductContainer>\n\n    );\n}\n\n\nexport default MyProduct;","import styled from \"styled-components\";\n\nexport const Button = styled.button`\n  width: 100%;\n  height: 100%;\n  text-align: center;\n  cursor: pointer;\n  background-color: #108CFF;\n  color: white;\n  border-radius: 10px;\n  font-size: 18px;\n  font-family: \"Roboto-Bold\";\n  border: none;\n\n`;\n\nexport default Button;\n","import React from \"react\";\nimport { Button as ButtonSt